{
  "name": "finance-tracker",
  "version": "1.0.0",
  "description": "Personal finance tracker with income/expense tracking and savings goals",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev --prefix server",
    "dev:client": "npm run dev --prefix client",
    "init-db": "npm run init-db --prefix server",
    "build": "npm run build --prefix client",
    "start": "npm start --prefix server"
  },
  "keywords": [
    "finance",
    "tracker",
    "budget",
    "personal-finance"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
